/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@core/*": [
        "app/core/*"
      ],
      "@services/*": [
        "app/core/services/*"
      ],
      "@interceptors/*": [
        "app/core/interceptors/*"
      ],
      "@guards/*": [
        "app/core/guards/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@models/*": [
        "app/shared/models/*"
      ],
      "@components/*": [
        "app/shared/components/*"
      ],
      "@admin/*": [
        "app/admin/*"
      ],
      "@auth/*": [
        "app/auth/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@environments/*": [
        "environments/*"
      ],
    }
  }
}
