{
  "name": "opportunity",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "local": "ng serve --proxy-config proxy/proxy.local.conf.json",
    "dev": "ng serve --proxy-config proxy/proxy.dev.conf.json",
    "live": "ng serve --proxy-config proxy/proxy.live.conf.json",
    "build": "ng build",
    "format:fix": "pretty-quick --staged",
    "pre-commit": "run-s format:fix lint",
    "lint": "ng lint",
    "dev:ssr": "ng run opportunity:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run opportunity:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/platform-server": "~11.2.11",
    "@angular/router": "~11.2.11",
    "@nguniversal/express-engine": "^11.0.1",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular-eslint/builder": "^4.0.0",
    "@angular-eslint/eslint-plugin": "^4.0.0",
    "@angular-eslint/eslint-plugin-template": "^4.0.0",
    "@angular-eslint/schematics": "^4.0.0",
    "@angular-eslint/template-parser": "^4.0.0",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.19.9",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^4.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  }
}
